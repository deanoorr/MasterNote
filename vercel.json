{
    "rewrites": [
        {
            "source": "/scira-api/:path*",
            "destination": "https://api.scira.ai/:path*"
        },
        {
            "source": "/deepseek-api/:path*",
            "destination": "https://api.deepseek.com/:path*"
        },
        {
            "source": "/openai-api/:path*",
            "destination": "https://api.openai.com/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}